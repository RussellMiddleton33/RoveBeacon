{
  "name": "threejs-user-location",
  "version": "1.0.0",
  "description": "A Three.js SDK for displaying user location with GPS accuracy and direction indicators",
  "type": "module",
  "main": "./dist/threejs-user-location.umd.cjs",
  "module": "./dist/threejs-user-location.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/threejs-user-location.js",
      "require": "./dist/threejs-user-location.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --config vite.lib.config.ts && tsc -p tsconfig.lib.json",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json"
  },
  "peerDependencies": {
    "three": ">=0.150.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.6",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "svelte": "^5.43.8",
    "svelte-check": "^4.3.4",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@types/three": "^0.181.0",
    "three": ">=0.150.0"
  },
  "keywords": [
    "three.js",
    "threejs",
    "geolocation",
    "gps",
    "map",
    "location",
    "user-marker",
    "webgl"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  }
}
